.. activecode:: gcd_py
    :author: bmiller
    :difficulty: 3
    :basecourse: cppds
    :topic: Introduction/ObjectOrientedProgrammingDefiningClasses
    :from_source: T
    :optional:

    def gcd(m,n):
        while m%n != 0:
            oldm = m
            oldn = n

            m = oldn
            n = oldm%oldn
        return n

    print(gcd(20,10))