   public class Test1
   {
       public static void main(String[] args)
       {
           int originalPrice = 68.00;
           int clearancePrice = originalPrice * 0.3;
           int finalPrice = clearancePrice * 0.8;
           System.out.println(finalPrice);
       }
   }