.. activecode:: DieExample
  :author: bmiller
  :difficulty: 3
  :basecourse: javareview
  :topic: OOBasics/objects&Classes
  :from_source: None
  :language: java

  public class Die
  {
     private int lastValue;

     public int roll()
     {
        lastValue = (int) (Math.random() * 6) + 1;
        return lastValue;
     }

     public static void main(String[] args)
     {
        Die d = new Die();
        for (int i = 0; i < 10; i++)
        {
           System.out.println(d.roll());
        }
     }
   }