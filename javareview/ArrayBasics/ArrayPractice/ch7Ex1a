   public class Test
   {
       public static void main(String[] args)
       {
           int[] arr1 = {1, 3, 7, 9, 15};
           for (int index = 0; index < arr1.length; index+=2)
           {
               System.out.print(arr1[index] + ", ");
           }
       }
   }