.. activecode:: example_trace_loop
   :author: bmiller
   :difficulty: 3
   :basecourse: javareview
   :topic: LoopBasics/lWhile
   :from_source: None
   :language: java

   public class Test
   {
       public static void main(String[] args)
       {
           int var1 = 3;
           int var2 = 2;

           while ((var2 != 0) && ((var1 / var2) >= 0))
           {
               var1 = var1 + 1;
               var2 = var2 - 1;
           }
       }
   }