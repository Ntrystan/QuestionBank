    :from_source: T
    :nocodelens:

    height = 60    # in inches (60 inches is 5 feet)
    weight = 110   # in pounds
    heightSquared = height * height
    BMI = weight / heightSquared
    BMImetric = BMI * 703
    print("BMI:")
    print(BMImetric)
