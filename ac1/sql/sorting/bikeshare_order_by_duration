.. activecode:: bikeshare_order_by_duration
   :author: bmiller
   :difficulty: 3
   :basecourse: ac1
   :topic: sql/sorting
   :from_source: T
   :language: sql
   :dburl: /runestone/books/published/ac1/_static/bikeshare.db

   SELECT
     *
   FROM
     trip_data
   ORDER BY
     duration DESC
   LIMIT
     10