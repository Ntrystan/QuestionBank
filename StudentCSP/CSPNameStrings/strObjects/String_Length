   :author: bmiller
   :difficulty: 3
   :basecourse: StudentCSP
   :topic: CSPNameStrings/strObjects
   :from_source: T
   :nocodelens:

   sentence = "This is a test"
   length = len(sentence)
   print(length)
   sentence = "Hi"
   length = len(sentence)
   print(length)