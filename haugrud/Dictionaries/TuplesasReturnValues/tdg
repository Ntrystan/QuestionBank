    def circleInfo(r):
        """ Return (circumference, area) of a circle of radius r """
        c = 2 * 3.14159 * r
        a = 3.14159 * r * r
        return (c, a)

    (circum, area) = circleInfo(10)
    print(circum, area)

    t = circleInfo(11)
    print(t)



