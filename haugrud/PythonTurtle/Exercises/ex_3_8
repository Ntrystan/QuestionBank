   import turtle
   wn = turtle.Screen()
   tess = turtle.Turtle()
   tess.speed(6)
   tess.right(90)
   tess.left(3600)
   tess.right(-90)
   tess.left(3600)
   tess.left(3645)
   tess.forward(-100)