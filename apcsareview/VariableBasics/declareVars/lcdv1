   public class Test1
   {
      public static void main(String[] args)
      {
        int score;
        score = 0;
        System.out.println(score);
        double price;
        price = 2.55;
        System.out.println(price);
        boolean won;
        won = false;
        System.out.println(won);
      }
   }
