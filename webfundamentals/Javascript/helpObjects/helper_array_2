   <body>
     <ul>
        <li>Luther</li>
        <li>Coe</li>
        <li>Simpson</li>
        <li>Central</li>
        <li>Wartburg</li>
     </ul>
     <script>
         theList = document.querySelectorAll('li')
         for(i=0; i< theList.length; i++) {
           alert(theList[i].innerText)
         }
     </script>
   </body>
   </html>

