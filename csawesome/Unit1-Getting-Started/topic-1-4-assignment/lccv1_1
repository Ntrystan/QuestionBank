   public class Test1
   {
      public static void main(String[] args)
      {
        //Please update the code so that score is printed out on one line with commas in between. 
        int score = 0;
        System.out.println(score);
        score = score + 1;
        System.out.println(score);
        score = score + 50;
        System.out.println(score);
      }
