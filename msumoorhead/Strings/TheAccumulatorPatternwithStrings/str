.. activecode:: str
    :author: jenkins
    :difficulty: 3
    :basecourse: msumoorhead
    :topic: Strings/TheAccumulatorPatternwithStrings
    :from_source: None

    def removeVowels(s):
        vowels = "aeiouAEIOU"
        newString = ""
        for aChar in s:
            if aChar not in vowels:
                newString = newString + aChar
        return newString

    print(removeVowels("compsci"))
    print(removeVowels("aAbEefIijOopUus"))