   title = ["Duplicity", "Notting Hill", "Pretty Woman", "Erin Brockovich", "Eat Pray Love", "Mona Lisa Smile"]
   year = (2009, 1999, 1990, 2000, 2010, 2003)

   for movie in zip(title, year):
       print(movie)