    L2 = ["Cherry", "Apple", "Blueberry"]

    L3 = sorted(L2)
    print(L3)
    print(sorted(L2))
    print(L2) # unchanged

    print("----")

    L2.sort()
    print(L2)
    print(L2.sort())  #return value is None