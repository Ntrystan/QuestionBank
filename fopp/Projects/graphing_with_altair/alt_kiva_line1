.. activecode:: alt_kiva_line1
    :author: bmiller
    :difficulty: 3.0
    :basecourse: fopp
    :topic: Projects/graphing_with_altair
    :from_source: T
    :nocodelens:

    import altair

    x_vals = range(-10, 11)
    y_vals = []
    for x in x_vals:
        y_vals.append(x*x)

    data = altair.Data(X=x_vals, Y=y_vals)
    chart = altair.Chart(data)
    mark = chart.mark_line()
    enc = mark.encode(x='X',y='Y')
    enc.display()
