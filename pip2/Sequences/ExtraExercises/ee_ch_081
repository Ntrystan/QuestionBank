   letts = ['a', 'b', 'b', 'c', 'd', 'e', 'e', 'e', 'f', 'f']

   =====

   from unittest.gui import TestCaseGui

   class myTests(TestCaseGui):

      def testOne(self):
         self.assertEqual(letts, ['a', 'b', 'b', 'c', 'd', 'f', 'f'], "Testing the list letts.")

   myTests().main()