.. activecode:: ch04_square
    :author: Brad Miller
    :difficulty: 3
    :basecourse: pip2
    :topic: Functions/Returningavaluefromafunction
    :from_source: None

    def square(x):
        y = x * x
        return y

    toSquare = 10
    result = square(toSquare)
    print "The result of " + str(toSquare) + " squared is " + str(result)