   info = "This book {} students {}.".format()

   =====

   from unittest.gui import TestCaseGui

   class myTests(TestCaseGui):

      def testOne(self):
         self.assertEqual(info, "This book teaches students python.", "Testing that info has the correct value.")

   myTests().main()