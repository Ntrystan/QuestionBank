   name = "Oreo"
   breed = "poodle"
   str1 = "This is {}, he is a {}."

   =====

   from unittest.gui import TestCaseGui

   class myTests(TestCaseGui):

      def testOne(self):
         self.assertEqual(str1, "This is Oreo, he is a poodle.", "Testing that str1 is assigned to correct value")

   myTests().main()