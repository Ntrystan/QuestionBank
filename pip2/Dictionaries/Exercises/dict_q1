   d = {'apples': 15, 'grapes': 12, 'bananas': 35}
   print d['banana']
   d['oranges'] = 20
   print len(d)
   print 'grapes' in d
   print d['pears']
   print d.get('pears', 0)
   fruits = d.keys()
   print fruits
   fruits.sort()
   print fruits
   del d['apples']
   print 'apples' in d