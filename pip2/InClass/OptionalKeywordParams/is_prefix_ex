    def is_prefix(s1,s2):
        return s2.find(s1) == 0

    #x = 5
    x = -2

    # To illustrate what we're doing with the boolean statement
    if x > 0:
        print True
    else:
        print False

    #That code does exactly the same thing as the following line of code
    print x > 0