    # here is a function count_vowels
    def count_vowels(s):
        vowels = ['a', 'e', 'i', 'o', 'u']
        ct=0
        for ch in s:
            if ch in vowels:
                ct = ct + 1
        return ct

    # Example invocation and print statement
    print count_vowels("supercalifragilisticexpialidocious")