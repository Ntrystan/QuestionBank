    x = [[1, 2, 3], [4, 5], [6, 7]]

    #With nested data, each level of nesting requires one level of extraction

    # With indexing

    y = x[1] #[4, 5]
    z = y[0]
    print z # 4

    # Same thing in one line
    w = x[1][0]
    print w # 4

    # or just
    print x[1][0]

    #OR printing pieces of nested data with iteration

    #for lst in x:
    #    print lst[0]

    #for lst in x:
    #    for item in lst:
    #        print item

    #print x
    #print x[1]
    #print type(x[1])

    #for item in x[1]:
    #    print item