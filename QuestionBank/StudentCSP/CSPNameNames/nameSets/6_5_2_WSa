    # DEFINE THE PROCEDURE
    def pentagon(turtle,size):
        turtle.forward(size)
        turtle.right(72)
        turtle.forward(size)
        turtle.right(72)
        turtle.forward(size)
        turtle.right(72)
        turtle.forward(size)
        turtle.right(72)
        turtle.forward(size)
        turtle.right(72)

    # CREATE TURTLE WORLD
    from turtle import *
    space = Screen()
    emily = Turtle()
    # CALL THE PROCEDURE
    pentagon(emily,100)