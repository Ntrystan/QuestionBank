   public class Test1
   {
       public static void main(String[] args)
       {
           String[] stArr1 = {"Destini", "Landon", "Anaya", "Gabby", "Evert"};
           for (int i = 1; i < stArr1.length; i+=2)
           {
               System.out.println(stArr1[i]);
           }
       }
   }