   import turtle,random
   wn = turtle.Screen()
   
   emma = turtle.Turtle()
   emma.shape("turtle")
   arnaud= turtle.Turtle()
   arnaud.shape("turtle")
   arnaud.penup()
   arnaud.goto(0,-10)
   arnaud.pendown()