    def rechthoek():
        alex=turtle.Turtle()
        alex.fd(100)
        alex.right(90)
        alex.fd(50)
        alex.right(90)
        alex.fd(100)
        alex.right(90)
        alex.fd(50)
        alex.right(90)

    import turtle
    rechthoek()