    def move(turtle):
        turtle.forward(100)
        turtle.right(90)
        turtle.forward(100)
        turtle.right(90)
        turtle.forward(100)
        turtle.right(90)
        turtle.forward(100)
        turtle.right(90)

    from turtle import *
    space = Screen()
    t = Turtle()
    move(t, 100, 90)