    inventory = {'apples': 430, 'bananas': 312, 'oranges': 525, 'pears': 217}
    print 'apples' in inventory
    print 'cherries' in inventory

    if 'bananas' in inventory:
        print inventory['bananas']
    else:
        print "We have no bananas"