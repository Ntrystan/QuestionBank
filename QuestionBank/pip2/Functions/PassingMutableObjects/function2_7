   def double(y):
       y = 2 * y

   def changeit(lst):
       lst[0] = "Michigan"
       lst[1] = "Wolverines"

   y = 5
   double(y)
   print y

   mylst = ['106', 'students', 'are', 'awesome']
   changeit(mylst)
   print mylst