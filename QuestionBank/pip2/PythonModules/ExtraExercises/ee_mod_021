   =====

   from unittest.gui import TestCaseGui

   class myTests(TestCaseGui):

      def testOne(self):
         self.assertEqual(adding, 13, "Testing that adding has the correct value.")
      def testTwo(self):
         self.assertEqual(multi, 42, "Testing that multi has the correct value.")


   myTests().main()