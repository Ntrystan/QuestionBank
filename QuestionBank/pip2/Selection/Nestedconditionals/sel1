    :basecourse: pip2
    :topic: Selection/Nestedconditionals
    :from_source: None
    :showoutput:

    x = 10
    y = 10

    if x < y:
        print "x is less than y"
    else:
        if x > y:
            print "x is greater than y"
        else:
            print "x and y must be equal"