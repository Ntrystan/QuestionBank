  /**
      Pet class (complete comments)
      @author
      @since

  */
  class Pet
  {
     // complete class definition with set methods

  }

  public class TesterClass
  {
     // main method for testing
     public static void main(String[] args)
     {
        // Create Pet objects and test all your set methods

     }
   }