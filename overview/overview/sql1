    :from_source: T
    :language: sql
    :autograde: unittest
    :dburl: https://runestone.academy/runestone/books/published/overview/_static/bikeshare.db

    select bike_number, max(duration)
    from trip_data
    where start_station = 31105
    group by bike_number
    order by max(duration) desc
    limit 4;

    ====
    assert 0,0 == W00554
    assert 0,1 == 51618
    assert 2,1 == 38347
