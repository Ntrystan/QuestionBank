    ~~~~
    import turtle
    wn =turtle.Screen()
    alex=turtle.Turtle()
    alex.forward(100)
    alex.right(60)
    alex.forward(100)
    alex.right(60)
    alex.forward(100)
    alex.right(60)
    alex.forward(100)
    alex.right(60)
    alex.forward(100)
    alex.right(60)