.. activecode:: ch09_join
    :author: bmiller
    :difficulty: 3.0
    :basecourse: thinkcspy
    :topic: Lists/StringsandLists
    :from_source: T

    wds = ["red", "blue", "green"]
    glue = ';'
    s = glue.join(wds)
    print(s)
    print(wds)

    print("***".join(wds))
    print("".join(wds))

