    ====
    from unittest.gui import TestCaseGui

    class myTests(TestCaseGui):
        def testOne(self):
            self.assertEqual(cipher('seat'), 'ties',  "Tested 'seat' ")
            self.assertEqual(cipher('bacon'), 'necub',  "Tested  'bacon' ")
            self.assertEqual(cipher('Allamakee'), 'illemekiA',  "Tested 'Allamakee' ")
            self.assertEqual(cipher('Louisville'), 'iuaosvollL',  "Tested 'Louisville' ")
            self.assertEqual(cipher('it'), 'to',  " 'to' ")
            self.assertEqual(cipher('a'), 'a',  " 'a' ")
           
          
    myTests().main()