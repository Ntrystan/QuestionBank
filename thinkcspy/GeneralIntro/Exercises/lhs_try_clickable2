.. clickablearea:: lhs_try_clickable2
    :author: LHS CS Team
    :difficulty: 0.0
    :basecourse: thinkcspy
    :topic: GeneralIntro/Exercises
    :from_source: F
    :question: Click anywhere you see a comment
    :iscode:
    :feedback: Comments start with a # symbol

    :click-correct: # my first program!!:endclick:
    :click-incorrect: :endclick:
    :click-correct: print('Hello World')   # say hi:endclick:
    :click-incorrect: a = 1:endclick:
    :click-incorrect: b = 2:endclick:
    :click-correct: a=b  # a should get the value of b:endclick:
    :click-incorrect: print(a):endclick: