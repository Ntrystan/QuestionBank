.. activecode::  ch7ex14a
    :author: bmiller
    :difficulty: 3.0
    :basecourse: TeacherCSP
    :topic: CSPRepeatNumbers/ch7_exercises
    :from_source: T
    :nocodelens:

    sum = 0
    numbers = range(1,20,2)
    for number in numbers:
            sum = sum + number
    average = int(sum / len(numbers))
    product = 1
    numbers2 = range(1,101, average)
    for number in numbers2:
            product = product * number
    print(product)